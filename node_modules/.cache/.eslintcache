his.props;\n    const { trackName, previewUrl, trackId } = music;\n    const { favorites, loading } = this.state;\n\n    if (previewUrl !== undefined) {\n      return (\n        <div className=\"music\">\n          <p>{trackName}</p>\n          <audio data-testid=\"audio-component\" src={previewUrl} controls>\n            <track kind=\"captions\" />O seu navegador não suporta o elemento{' '}\n            <code>audio</code>\n          </audio>\n          <label htmlFor={trackId}>\n            Favorita\n            <input\n              type=\"checkbox\"\n              data-testid={`checkbox-music-${trackId}`}\n              onChange={this.handleCange}\n              checked={favorites.some((song) => song.trackId === trackId)}\n              id={trackId}\n            />\n          </label>\n          {loading ? <Loading byFavorites=\"true\" /> : null}\n        </div>\n      );\n    }\n    return null\n  }\n\n  render() {\n    const { music } = this.props;\n\n    const { trackName, previewUrl, trackId } = music;\n\n    const { favorites, loading } = this.state;\n    return this.renderPage();\n  }\n}\n\nMusicCard.propTypes = {\n  music: PropTypes.arrayOf(Object).isRequired,\n  updateFavorites: PropTypes.func,\n};\n\nMusicCard.defaultProps = {\n  updateFavorites: undefined,\n};\n","/home/leonardo/Área de Trabalho/Repositorios/sd-016-b-project-trybetunes/src/components/Header.jsx",[],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/index.js",[],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/App.js",[],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/pages/Login.jsx",[],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/pages/Search.jsx",["212"],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/pages/Album.jsx",["213"],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/pages/Favorites.jsx",[],"/home/leonardo/Área de Trabalho/Repositorios/TrybeTunes/src/pages/ProfileEdit.jsx",[],"/home/leonardo/Área de Tra